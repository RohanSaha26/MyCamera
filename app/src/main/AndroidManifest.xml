        android:icon="@drawable/iconcamera"
        android:label="@string/app_name"
        android:roundIcon="@drawable/iconcamera"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyCamera"
        tools:targetApi="31" >
        <activity
            android:name=".ImageEdit"
            android:exported="true" >

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".ImageProcess"
